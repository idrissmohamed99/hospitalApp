@model hospitalApp.Models.ViewModels.CheckUpViewModel
@{
    ViewBag.Title = "حذف كشف";
}

<div>
    <h4>كشف</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayName("اسم المريض")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Patientname)
        </dd>

        <dt>
            @Html.DisplayName("اسم الطبيب")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.doctorName)
        </dd>
        <dt>
            @Html.DisplayName(" نوع الكشف")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.CheckType)
        </dd>

    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-default" /> |
            @Html.ActionLink("Back to List", "GetChecksUp")
        </div>
    }
</div>


